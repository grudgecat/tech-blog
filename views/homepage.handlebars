{{#each posts as |post|}}
  <div class="row">
    <div class="col s12">
      <div class="card">
        <div class="card-content white-text">
          <div class='blogTitle'><span class="card-title">{{postTitle}} </span><span class="card-title">{{ format_date postDate}}</span></div>
          <p>{{postText}}</p>
        </div>
        <div class="card-action">
          <a href="#">Leave a Comment</a>
        </div>
      </div>
    </div>
  </div>
{{/each}}